{
    "name": "mediashare/time-tracking",
    "description": "Time-Tracking is a simple command line tool for project timer management.",
    "type": "symfony-bundle",
    "keywords": ["timer","time tracking","versionning timer","console timer", "project time management", "project management", "time management", "command line tool", "cmd"],
    "homepage": "https://github.com/Mediashare/time-tracking",
    "license": "MIT",
    "authors": [
        {
            "name": "Thibault Marquand",
            "email": "Mediashare.supp@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/cache": "^6.3",
        "symfony/console": "^6.3",
        "symfony/dotenv": "^6.3",
        "symfony/flex": "*",
        "symfony/framework-bundle": "^6.3",
        "symfony/yaml": "^6.3"
    },
    "bin": ["time-tracking"],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "../../bin",
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "Mediashare\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "console-package": "php bin/time-tracking"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "bin": [
        "bin/console",
        "bin/time-tracking"
    ],
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.3",
            "console": "bin/console"
        },
        "time-tracking": {
            "console": "bin/time-tracking"
        }
    }
}
